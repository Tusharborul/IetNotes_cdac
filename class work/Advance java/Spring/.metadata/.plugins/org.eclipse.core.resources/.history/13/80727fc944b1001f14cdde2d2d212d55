package com.demo.beans;

public class Order {
	private int Oid;
	private Product p;
	public Order() {
		super();
	}
	public Order(int oid, Product p) {
		super();
		Oid = oid;
		this.p = p;
	}
	public int getOid() {
		return Oid;
	}
	public void setOid(int oid) {
		Oid = oid;
	}
	public Product getP() {
		return p;
	}
	public void setP(Product p) {
		this.p = p;
	}
	@Override
	public String toString() {
		return "Order [Oid=" + Oid + ", p=" + p + "]";
	}
	
	
	

}
